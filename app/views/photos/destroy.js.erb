$('#photos').html("<%= j render 'photos_list' %>")
<% if @event.photos.blank? %>
$('#photo_check').hide();
<% end %>

<% if !@event.active && !@event.price.blank? && !@event.listing_name.blank? && !@event.photos.blank? && !@event.address.blank? %>
$('#publish_button').attr('disabled', false);
<% else %>
$('#publish_button').attr('disabled', true);
<% end %>
